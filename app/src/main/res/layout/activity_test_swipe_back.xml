<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_test_swipe_back"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorContent2"
    tools:context="com.itisi.itisiapp.ui.main.TestSwipeBackActivity">

    <!--android:fitsSystemWindows="true"-->
    <!--android:clipToPadding="false"-->
    <ImageView
        android:id="@+id/iv_fuck"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@mipmap/fuck"
        />
    <TextView
        android:id="@+id/tv_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="textswipe"
        android:layout_below="@id/iv_fuck"
        />


    <Button
        android:layout_below="@id/tv_test"
        android:id="@+id/btn_toast2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="testapp"
        />
</RelativeLayout>
